# Foxy Confidential - Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# ========================================
# GOOGLE MAPS & PLACES API
# ========================================
# Get your API key from: https://console.cloud.google.com/apis/credentials
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# ========================================
# BACKEND API CONFIGURATION
# ========================================
# Backend server URL (default for local development)
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=10000

# ========================================
# AUTHENTICATION
# ========================================
# JWT secret for token encryption (generate a strong random string)
REACT_APP_JWT_SECRET=your_jwt_secret_key_here_minimum_32_characters

# ========================================
# FEATURE FLAGS
# ========================================
# Enable/disable features
REACT_APP_USE_MOCK_DATA=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false

# ========================================
# APP CONFIGURATION
# ========================================
REACT_APP_NAME=Foxy Confidential
REACT_APP_VERSION=0.1.0
REACT_APP_ENV=development

# ========================================
# THIRD-PARTY SERVICES (Optional)
# ========================================
# Analytics (e.g., Google Analytics)
REACT_APP_GA_TRACKING_ID=

# Error tracking (e.g., Sentry)
REACT_APP_SENTRY_DSN=

# ========================================
# PWA CONFIGURATION
# ========================================
REACT_APP_PWA_NAME=Foxy Confidential
REACT_APP_PWA_SHORT_NAME=FoxyC
REACT_APP_PWA_THEME_COLOR=#667eea

